      <HintPath>libs/Straftat/GameKit.Utilities.dll</HintPath>
    </Reference>
    <Reference Include="GrabbitAssembly">
      <HintPath>libs/Straftat/GrabbitAssembly.dll</HintPath>
    </Reference>
    <Reference Include="Heathen.Core">
      <HintPath>libs/Straftat/Heathen.Core.dll</HintPath>
    </Reference>
    <Reference Include="Heathen.PhysKit">
      <HintPath>libs/Straftat/Heathen.PhysKit.dll</HintPath>
    </Reference>
    <Reference Include="Heathen.PhysKit.Examples">
      <HintPath>libs/Straftat/Heathen.PhysKit.Examples.dll</HintPath>
    </Reference>
    <Reference Include="Heathen.Steamworks">
      <HintPath>libs/Straftat/Heathen.Steamworks.dll</HintPath>
    </Reference>
    <Reference Include="Heathen.Steamworks.Examples">
      <HintPath>libs/Straftat/Heathen.Steamworks.Examples.dll</HintPath>
    </Reference>
    <Reference Include="LambdaTheDev.NetworkAudio.Sync">
      <HintPath>libs/Straftat/LambdaTheDev.NetworkAudio.Sync.dll</HintPath>
    </Reference>
    <Reference Include="LambdaTheDev.NetworkAudio.Sync.Demo.FishNet">
      <HintPath>libs/Straftat/LambdaTheDev.NetworkAudio.Sync.Demo.FishNet.dll</HintPath>
